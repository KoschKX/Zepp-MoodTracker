{
  "configVersion": "v2",
  "app": {
    "appId": 27867,
    "appName": "Mood Tracker",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "Mood Tracing App to help you monitor and improve your mental well-being."
  },
  "permissions": [
    "data:os.device.info",
    "device:os.local_storage",
    "data:user.hd.calorie",
    "device:os.bg_service",
    "device:os.ble"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "2.0.0",
      "target": "2.0.0",
      "minVersion": "2.0.0"
    }
  },
  "targets": {
    "416x416-amazfit-gtr-mini": {
      "module": {
        "page": {
          "pages": [
            "page/sync_page",
            "page/index",
            "page/mood_page",
            "page/week_page",
            "page/month_page",
            "page/mood_select"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        }
      },
      "platforms": [
        {
          "name": "LeidenW",
          "deviceSource": 251
        }
      ],
      "designWidth": 416
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Mood Tracker"
    }
  },
  "defaultLanguage": "en-US"
}
